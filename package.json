{
  "name": "es6-js-boilerplate",
  "version": "1.0.0",
  "description": "ES6 JavaScript for writing Node.js",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist && babel src --out-dir dist  --copy-files",
